<body>
    <div class="page">
        <section>
            <div class="comparador">
                <div class="container">
                    <div class="row h-12">
                        <div class="col-12">
                            <h2>Comparador</h2>
                        </div>
                        <div class="col-6">
                            <label for="pecas1">Peças</label>
                            <select name="pecas1" id="pecas1">
                                    <option value="0" selected>-</option>
                                    <option name="id" value="111">Celeron G5900</option>
                                    <option name="id" value="121">Pentium® Gold G6500T</option>
                                    <option name="id" value="131">Core™2 Duo E8400</option>
                                    <option name="id" value="21">i3-7100</option>
                                    <option name="id" value="31">i3-8100</option>
                                    <option name="id" value="1">i3-9350KF</option>
                                    <option name="id" value="41">i5-7400</option>
                                    <option name="id" value="51">i5-8400</option>
                                    <option name="id" value="11">i5-9400F</option>
                                    <option name="id" value="61">i5-9600K</option>
                                    <option name="id" value="71">i7-7700K</option>
                                    <option name="id" value="81">i7-8700</option>
                                    <option name="id" value="91">i7-8700K</option>
                                    <option name="id" value="101">i7-9700F</option>
                            </select>

                        </div>
                        <div class="col-6">
                            <label for="pecas2">Peças</label>
                            <select name="pecas2" id="pecas2">
                                    <option value="0" selected>-</option>
                                    <option name="id" value="111">Celeron G5900</option>
                                    <option name="id" value="121">Pentium® Gold G6500T</option>
                                    <option name="id" value="131">Core™2 Duo E8400</option>
                                    <option name="id" value="21">i3-7100</option>
                                    <option name="id" value="31">i3-8100</option>
                                    <option name="id" value="1">i3-9350KF</option>
                                    <option name="id" value="41">i5-7400</option>
                                    <option name="id" value="51">i5-8400</option>
                                    <option name="id" value="11">i5-9400F</option>
                                    <option name="id" value="61">i5-9600K</option>
                                    <option name="id" value="71">i7-7700K</option>
                                    <option name="id" value="81">i7-8700</option>
                                    <option name="id" value="91">i7-8700K</option>
                                    <option name="id" value="101">i7-9700F</option>
                            </select>
                        </div>
                        <div class="col-6">
                          {{# each SQLarray}}
                            <div class="especificacoes">
                                <h1>i3-9350KF</h1>
                                <h2>Thread</h2>
                                <p>{{ this.valorCaracteristica1 }}</p>
                                <h2>Clock</h2>
                                <p>Até {{ this.valorCaracteristica2 }} GHz</p>
                                <h2>Soquete</h2>
                                <p>{{ this.valorCaracteristica3 }}</p>
                                <h2>TDP</h2>
                                <p>{{ this.valorCaracteristica4 }} W</p>
                                <h2>Cache</h2>
                                <p>{{ this.valorCaracteristica5 }} Mb</p>
                            </div>
                            {{/each}}
                        </div>
                        <div class="col-6">
                            <div class="especificacoes">
                                <h1>---</h1>
                                <h2>Thread</h2>
                                <p></p>
                                <h2>Clock</h2>
                                <p>Até  GHz</p>
                                <h2>Soquete</h2>
                                <p></p>
                                <h2>TDP</h2>
                                <p> W</p>
                                <h2>Cache</h2>
                                <p> Mb</p>
                            </div>
                        </div>
                        <div class="col-12">
                            <div class="conteudo">
                                <div class="carroceu">
                                    <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
                                        <ol class="carousel-indicators">
                                            <li data-target="#carouselExampleIndicators" data-slide-to="0"
                                                class="active">
                                            </li>
                                            <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
                                            <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
                                        </ol>
                                        <div class="carousel-inner">
                                            <div class="carousel-item active">
                                                <img class="d-block w-100" src="/img/1.png" alt="First slide">

                                            </div>
                                            <div class="carousel-item">
                                                <img class="d-block w-100" src="/img/2.png" alt="Second slide">
                                            </div>
                                            <div class="carousel-item">
                                                <img class="d-block w-100" src="/img/3.png" alt="Third slide">
                                            </div>
                                        </div>
                                        <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button"
                                            data-slide="prev">
                                            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                                            <span class="sr-only">Previous</span>
                                        </a>
                                        <a class="carousel-control-next" href="#carouselExampleIndicators" role="button"
                                            data-slide="next">
                                            <span class="carousel-control-next-icon" aria-hidden="true"></span>
                                            <span class="sr-only">Next</span>
                                        </a>
                                    </div>
                                    <hr style="height:2px;border-width:0;color:snow;background-color:snow">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
        </section>
